    "type": "utility",
    "require": {
        "php": "^7.2.5",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/events": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Iatstuti\\Database\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
